{
    "name": "My First Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "The first extension that I made.",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": ["jquery.js"]
        },
        {
            "matches": ["http://www.treasuretrooper.com/members/earn/offers*"],
            "js": ["treasuretrooper.js"]
        }
    ],
    "permissions": [
        "http://*/*"
    ]
}