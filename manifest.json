{
    "name": "__MSG_manifestTitle__",
    "version": "1.0",
    "manifest_version": 2,
    "description": "__MSG_manifestDescription__",
    "browser_action": {
        "default_icon": "img/icon_bw_48.png",
        "default_title": "__MSG_popupTitleNotReady__",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/icon_16.png",
        "48": "img/icon_48.png",
        "128": "img/icon_128.png"
    },
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "default_locale": "en",
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": [
                "js/vendor/jquery.js",
                "js/vendor/drm.js",
                "js/Evt.js",
                "js/Registry.js",
                "js/Url.js",
                "js/Gpt/Treasuretrooper.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "history",
        "storage",
        "management",
        "http://*/*"
    ]
}