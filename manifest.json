{
    "name": "GPT Do Nothing",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Get paid to be lazy.",
    "page_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": ["http://*/*"],
            "js": [
                "js/vendor/jquery.js",
                "js/Evt.js",
                "js/Registry.js",
                "js/Url.js",
                "init.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "history",
        "storage",
        "http://*/*"
    ]
}